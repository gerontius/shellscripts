# commands for aws cli setup and function on ubuntu


apt-get install awscli
aws configure
aws s3 ls
aws s3api create-bucket --bucket ananzsitehosts --region eu-west-2 --create-bucket-configuration LocationConstraint=eu-west-2
aws s3 cp ananzsites1.tar.gz s3://ananzsitehosts
aws s3 ls s3://ananzsitehosts

